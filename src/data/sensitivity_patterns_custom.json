        "name": "Stock Options/Equity Column",
        "pattern": "\\b(?:rsu|stock_grant|equity_grant|vesting|options_granted)\\b",
        "description": "Column names for stock options and equity grants",
        "categories": [
          "financial",
          "organizational",
          "column_name"
        ],
        "examples": [
          "RSU_granted",
          "stock_grant_date",
          "vesting_schedule"
        ]
      },
      {
        "id": "azure_connection_key",
        "name": "Azure Connection String with Key",
        "pattern": "\\bAccountKey\\s*=\\s*[A-Za-z0-9+/]{40,}={0,2}",
        "description": "Azure storage account key in connection string",
        "categories": [
          "credentials",
          "security",
          "azure",
          "cloud"
        ],
        "examples": [
          "AccountKey=dGVzdGtleWJhc2U2NGVuY29kZWQ="
        ]
      },
      {
        "id": "aws_credentials",
        "name": "AWS Credentials",
        "pattern": "\\b(?:AKIA[0-9A-Z]{16}|aws_secret_access_key)\\b",
        "description": "AWS access key ID and secret access key patterns",
        "categories": [
          "credentials",
          "security",
          "aws",
          "cloud"
        ],
        "examples": [
          "AKIAIOSFODNN7EXAMPLE",
          "aws_secret_access_key=wJalrXUtnFEMI"
        ]
      },
      {
        "id": "jwt_token",
        "name": "JWT Token",
        "pattern": "\\beyJ[A-Za-z0-9_-]{10,}\\.[A-Za-z0-9_-]{10,}\\.[A-Za-z0-9_-]{10,}\\b",
        "description": "JSON Web Token (JWT) pattern",
        "categories": [
          "credentials",
          "security",
          "api"
        ],
        "examples": [
          "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJzdWIiOiIxMjM0NTY3ODkwIn0.dozjgNryP4J3jVmNHl0w5N_XgL0n3I9PlFUP0THsR8U"
        ]
      },
      {
        "id": "mac_address",
        "name": "MAC Address",
        "pattern": "\\b(?:[0-9A-Fa-f]{2}[:-]){5}[0-9A-Fa-f]{2}\\b",
        "description": "Network MAC address pattern",
        "categories": [
          "infrastructure",
          "network"
        ],
        "examples": [
          "00:1B:44:11:3A:B7",
          "00-1B-44-11-3A-B7"
        ]
      },
      {
        "id": "bob_test",
        "name": "Testing for Bobs Name",
        "pattern": "(?i)BOB",
        "description": "Looks for Bob",
        "categories": [],
        "examples": [
          "Bob"
        ]
      }
    ],
