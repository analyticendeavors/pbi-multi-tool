          "column_name"
        ],
        "examples": [
          "username",
          "user_name",
          "login_id"
        ]
      },
      {
        "id": "server_name",
        "name": "Server Name",
        "pattern": "\\b(?:server|srv|sql|db)[\\w-]*\\d+\\b",
        "description": "Server naming pattern (e.g., server01, sqldb-prod)",
        "categories": [
          "infrastructure"
        ],
        "examples": [
          "server01",
          "sqldb-prod",
          "db-server-123"
        ]
      },
      {
        "id": "ip_address",
        "name": "IP Address",
        "pattern": "\\b(?:\\d{1,3}\\.){3}\\d{1,3}\\b",
        "description": "IPv4 address pattern",
        "categories": [
          "infrastructure"
        ],
        "examples": [
          "192.168.1.1",
          "10.0.0.50"
        ]
      },
      {
        "id": "file_path_windows",
        "name": "Windows File Path",
        "pattern": "[A-Za-z]:\\\\[^\\s]+",
        "description": "Windows file system path",
        "categories": [
          "infrastructure",
          "file_path"
        ],
        "examples": [
          "C:\\Users\\Documents\\file.txt",
          "D:\\Data\\reports"
        ]
      },
      {
        "id": "unc_path",
        "name": "UNC Network Path",
        "pattern": "\\\\\\\\[\\w.-]+\\\\[^\\s]+",
        "description": "UNC network path pattern",
        "categories": [
          "infrastructure",
          "file_path"
        ],
        "examples": [
          "\\\\server\\share\\folder",
          "\\\\fileserver01\\data"
        ]
      },
      {
        "id": "rls_expression",
        "name": "Row-Level Security Expression",
        "pattern": "(?:USERNAME|USERPRINCIPALNAME)\\s*\\(\\s*\\)",
        "description": "DAX RLS function usage",
        "categories": [
          "security",
          "dax"
        ],
        "examples": [
          "USERNAME()",
          "USERPRINCIPALNAME()"
        ]
      },
      {
        "id": "connection_string",
        "name": "Connection String",
        "pattern": "(?:Data Source|Server|Initial Catalog|Database)\\s*=\\s*[^;\"']+",
        "description": "Connection string components",
        "categories": [
          "connection_string",
          "infrastructure"
        ],
        "examples": [
          "Data Source=server;",
          "Initial Catalog=mydb;"
        ]
      },
      {
        "id": "biometric_column",
        "name": "Biometric Data Column",
        "pattern": "\\b(?:biometric|fingerprint|facial_recognition|retina|iris|voiceprint)\\b",
        "description": "Column names suggesting biometric data",
