  "whitelist": {
    "description": "Patterns to exclude from findings (false positives)",
    "patterns": [
      {
        "pattern": "\\b000-00-0000\\b",
        "description": "Placeholder SSN"
      },
      {
        "pattern": "\\b123-45-6789\\b",
        "description": "Example SSN"
      },
      {
        "pattern": "\\b(?:0000[\\s-]?){3}0000\\b",
        "description": "Placeholder credit card"
      },
      {
        "pattern": "\\bexample\\.com\\b",
        "description": "Example domain"
      },
      {
        "pattern": "\\btest@test\\.com\\b",
        "description": "Test email"
      },
      {
        "pattern": "\\b(?:127\\.0\\.0\\.1|localhost)\\b",
        "description": "Localhost references"
      },
      {
        "pattern": "\\b(?:xxx|yyy|zzz)\\b",
        "description": "Placeholder text"
      }
    ]
  },
