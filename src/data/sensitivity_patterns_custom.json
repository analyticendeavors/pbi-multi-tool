{
  "version": "1.0.0",
  "description": "Sensitivity patterns for Power BI semantic model scanning",
  "last_updated": "2025-11-17",
  "patterns": {
    "high_risk": [
      {
        "id": "ssn_us",
        "name": "US Social Security Number",
        "pattern": "\\b\\d{3}-\\d{2}-\\d{4}\\b",
        "description": "US Social Security Number in format XXX-XX-XXXX",
        "categories": [
          "pii",
          "government_id"
        ],
        "examples": [
          "123-45-6789",
          "987-65-4321"
        ]
      },
      {
        "id": "ssn_column",
        "name": "SSN Column Name",
        "pattern": "\\b(?:ssn|social_security|socialsecurity|social_sec|ssn_number)\\b",
        "description": "Column name suggesting SSN data",
        "categories": [
          "pii",
          "government_id",
          "column_name"
        ],
        "examples": [
          "SSN",
          "social_security_number",
          "SocialSecurityNo"
        ]
      },
      {
        "id": "credit_card",
        "name": "Credit Card Number",
        "pattern": "\\b(?:\\d{4}[\\s-]?){3}\\d{4}\\b",
        "description": "Credit card number pattern (16 digits)",
        "categories": [
          "financial",
          "pii"
        ],
        "examples": [
          "1234-5678-9012-3456",
          "1234 5678 9012 3456"
        ]
      },
      {
        "id": "credit_card_column",
        "name": "Credit Card Column Name",
        "pattern": "\\b(?:credit_card|creditcard|card_number|cardnumber|cc_number|ccnumber)\\b",
        "description": "Column name suggesting credit card data",
        "categories": [
          "financial",
          "pii",
          "column_name"
        ],
        "examples": [
          "credit_card_number",
          "CreditCardNo",
          "card_num"
        ]
      },
      {
        "id": "api_key",
        "name": "API Key",
        "pattern": "\\b(?:api[_-]?key|apikey|access[_-]?key|accesskey)[\\s:=\"']+[A-Za-z0-9\\-_]{20,}",
        "description": "API key pattern with key prefix",
        "categories": [
          "credentials",
          "security"
        ],
        "examples": [
          "api_key=abc123xyz789def456",
          "API_KEY: abc123xyz789"
        ]
      },
      {
        "id": "password",
        "name": "Password",
        "pattern": "\\b(?:password|passwd|pwd)[\\s:=\"']+[^\\s;\"']{8,}",
        "description": "Password pattern with pwd prefix",
        "categories": [
          "credentials",
          "security"
        ],
        "examples": [
          "password=MyP@ssw0rd",
          "Password: SecurePass123"
        ]
      },
      {
        "id": "connection_string_creds",
        "name": "Connection String with Credentials",
        "pattern": "(?:User ID|UID|Password|PWD)\\s*=\\s*[^;\"']+",
        "description": "Connection string containing user ID or password",
        "categories": [
          "credentials",
          "connection_string",
          "security"
        ],
        "examples": [
          "User ID=admin;Password=secret",
          "UID=sa;PWD=pass"
        ]
      },
      {
        "id": "email_address",
        "name": "Email Address",
        "pattern": "\\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Z|a-z]{2,}\\b",
        "description": "Email address pattern",
        "categories": [
          "pii",
          "contact"
        ],
        "examples": [
          "user@example.com",
          "john.doe@company.org"
        ]
      },
      {
        "id": "phone_us",
        "name": "US Phone Number",
        "pattern": "\\b(?:\\+?1[\\s.-]?)?\\(?\\d{3}\\)?[\\s.-]?\\d{3}[\\s.-]?\\d{4}\\b",
        "description": "US phone number in various formats",
        "categories": [
          "pii",
          "contact"
        ],
        "examples": [
          "(555) 123-4567",
          "555-123-4567",
          "+1-555-123-4567"
        ]
      },
      {
        "id": "tin_ein",
        "name": "Tax Identification Number (TIN/EIN/ITIN)",
        "pattern": "\\b(?:TIN|EIN|ITIN)[\\s:=#]*\\d{2}-?\\d{7}\\b",
        "description": "US Tax ID numbers (TIN, EIN, ITIN)",
        "categories": [
          "financial",
          "pii",
          "government_id"
        ],
        "examples": [
          "EIN: 12-3456789",
          "TIN 12-3456789",
          "ITIN 91-2345678"
        ]
      },
      {
        "id": "passport_number",
        "name": "Passport Number",
        "pattern": "\\b(?:passport|passport_number|passport_no)[\\s:=#]*[A-Z0-9]{6,9}\\b",
        "description": "International passport number patterns",
        "categories": [
          "pii",
          "government_id",
          "travel"
        ],
        "examples": [
          "Passport: A12345678",
          "passport_no=N1234567"
        ]
      },
      {
        "id": "drivers_license",
        "name": "Driver's License Number",
        "pattern": "\\b(?:drivers_license|driver_license|dl_number|license_number)[\\s:=#]*[A-Z0-9]{5,12}\\b",
        "description": "Driver's license number patterns (US states)",
        "categories": [
          "pii",
          "government_id"
        ],
        "examples": [
          "DL_Number: A1234567",
          "drivers_license=D12345678"
        ]
      },
      {
        "id": "bank_account",
        "name": "Bank Account Number",
        "pattern": "\\b(?:account_number|account_no|bank_account|acct_num)[\\s:=#]*\\d{8,17}\\b",
        "description": "Bank account number patterns",
        "categories": [
          "financial",
          "pii"
        ],
        "examples": [
          "account_number: 123456789012",
          "bank_account=12345678"
        ]
      },
      {
        "id": "iban",
        "name": "IBAN (International Bank Account Number)",
        "pattern": "\\b[A-Z]{2}\\d{2}[A-Z0-9]{11,30}\\b",
        "description": "International Bank Account Number",
        "categories": [
          "financial",
          "pii"
        ],
        "examples": [
          "GB82WEST12345698765432",
          "DE89370400440532013000"
        ]
      },
      {
        "id": "routing_number",
        "name": "Bank Routing Number",
        "pattern": "\\b(?:routing|routing_number|aba)[\\s:=#]*\\d{9}\\b",
        "description": "US Bank routing number (ABA)",
        "categories": [
          "financial"
        ],
        "examples": [
          "routing: 123456789",
          "ABA=021000021"
        ]
      },
      {
        "id": "medical_record",
        "name": "Medical Record Number (MRN)",
        "pattern": "\\b(?:mrn|medical_record|patient_id|medical_id)[\\s:=#]*[A-Z0-9]{6,12}\\b",
        "description": "Medical record and patient ID numbers",
        "categories": [
          "pii",
          "medical",
          "hipaa"
        ],
        "examples": [
          "MRN: 12345678",
          "patient_id=MED123456"
        ]
      },
      {
        "id": "national_id",
        "name": "National ID Number",
        "pattern": "\\b(?:national_id|citizen_id|govt_id|government_id)[\\s:=#]*[A-Z0-9]{6,15}\\b",
        "description": "Government-issued national ID numbers",
        "categories": [
          "pii",
          "government_id"
        ],
        "examples": [
          "national_id: ABC123456",
          "citizen_id=12345678901"
        ]
      },
      {
        "id": "oauth_token",
        "name": "OAuth/Bearer Token",
        "pattern": "\\b(?:bearer|oauth)[\\s:=#]*[A-Za-z0-9\\-_]{30,}\\b",
        "description": "OAuth and Bearer authentication tokens",
        "categories": [
          "credentials",
          "security",
          "api"
        ],
        "examples": [
          "Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9",
          "oauth_token=abc123xyz789"
        ]
      },
      {
        "id": "private_key",
        "name": "Private/SSH Key",
        "pattern": "-----BEGIN (?:RSA |EC |DSA |OPENSSH )?PRIVATE KEY-----",
        "description": "Private key file headers (SSH, RSA, etc.)",
        "categories": [
          "credentials",
          "security",
          "encryption"
        ],
        "examples": [
          "-----BEGIN RSA PRIVATE KEY-----",
          "-----BEGIN PRIVATE KEY-----"
        ]
      },
      {
        "id": "encryption_key",
        "name": "Encryption Key",
        "pattern": "\\b(?:encryption_key|secret_key|private_key|symmetric_key)[\\s:=#]*[A-Za-z0-9+/]{32,}={0,2}\\b",
        "description": "Encryption keys (base64 encoded)",
        "categories": [
          "credentials",
          "security",
          "encryption"
        ],
        "examples": [
          "encryption_key=dGVzdGtleWJhc2U2NGVuY29kZWQ=",
          "secret_key: abc123xyz789base64key"
        ]
      }
    ],
    "medium_risk": [
      {
        "id": "salary_column",
        "name": "Salary/Compensation Column",
